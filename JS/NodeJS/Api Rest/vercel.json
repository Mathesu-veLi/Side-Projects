{
  "rewrites": [{ "source": "/(.*)", "destination": "/src" }],
  "version": 2,
    "name": "api-rest",
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/src/(.*)",
            "dest": "app.js"
        }
    ]
}
